programa {
  funcao inicio() {
    inteiro n1,n2,n3,resultado
    escreva ("Informe um número: ")
    leia(n1)
    escreva ("Informe um segundo número: ")
    leia(n2)
    escreva ("Informe um terceiro número: ")
    leia(n3)
    resultado = (n1+n3)*n2
    escreva("O resultado dos 3 números escolhidos é: ",resultado)
  }
}
