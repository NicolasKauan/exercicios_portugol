programa {
  funcao inicio() {
    real n1,n2,n3,media
    escreva ("Informe um número: ")
    leia(n1)
    escreva ("Informe um segundo número: ")
    leia(n2)
    escreva ("Informe um terceiro número: ")
    leia(n3)
    media = (n1+n2+n3)/3
    escreva("A media dos 3 números escolhidos é: ",media)
  }
}
